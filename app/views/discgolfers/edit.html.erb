<%= form_with url: "/discgolfers/#{@discgolfer.id}", method: :patch, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :course %>
  <%= form.text_field :rating %>
  <%= form.label :pdga_member %>
  <%= form.check_box :pdga_member%>

  <%= form.submit "Update Discgolfer" %>
<% end %>
