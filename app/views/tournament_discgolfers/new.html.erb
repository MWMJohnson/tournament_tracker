<%= form_with url: "/tournaments/#{@tournament.id}/discgolfers", method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :rating %>
  <%= form.text_field :rating %>
  <%= form.label :pdga_member %>
  <%= form.check_box :pdga_member %>

  <%= form.submit "Create Discgolfer" %>
<% end %>